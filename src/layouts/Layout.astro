---
interface Props {
	title: string;
}
import '../styles/global.css'
const { title } = Astro.props;
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="canonical" href="https://scriptogres.vercel.app/">
		<link rel="icon" type="image/svg+xml" href="../icons/scriptogre-logo.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body class="bg-gray-100" >
		<slot />
	</body>
</html>
